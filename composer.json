{
  "name": "thomasvargiu/pami-module",
  "description": "Laminas module for PAMI Asterisk Library",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/thomasvargiu/pami-module",
  "keywords": [
    "zf2",
    "zend framework",
    "laminas",
    "mezzio",
    "pami",
    "asterisk",
    "ami",
    "asterisk manager interface"
  ],
  "authors": [
    {
      "name": "Thomas Vargiu",
      "email": "tvargiu@gmail.com",
      "homepage": "http://thomasvargiu.com/"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "marcelog/pami": "^2.0",
    "laminas/laminas-servicemanager": "^3.0",
    "psr/log": "^1.1",
    "psr/event-dispatcher": "^1.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpstan/phpstan": "^0.12.29"
  },
  "autoload": {
    "psr-4": {
      "PamiModule\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PamiModuleTest\\": "test/unit/"
    }
  },
  "extra": {
    "laminas": {
      "component": "PamiModule",
      "config-provider": "PamiModule\\ConfigProvider"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-clover clover.xml"
  }
}
